function GetDAZzleAppDataFolder(Value: string): string;
var
  AppDataPath: string;
begin
  // initialize default path, which will be returned when the following registry
  // key queries fail due to missing keys or for some different reason
  Result := '{userappdata}\Endicia\DAZzle';
  // query the first registry value; if this succeeds, return the obtained value
  if RegQueryStringValue(HKCU, 'Software\Envelope Manager\dazzle\Preferences', 'AppDataDirectory', AppDataPath) then
    Result := AppDataPath
  else
    if RegQueryStringValue(HKLM, 'Software\Envelope Manager\dazzle', 'AppDataDirectory', AppDataPath) then
      Result := AppDataPath
    else
      if RegQueryStringValue(HKLM, 'Software\Envelope Manager\dazzle', 'AppDataDirectory', AppDataPath) then
        Result := AppDataPath
      else
        if RegQueryStringValue(HKLM, 'Software\Wow6432Node\Envelope Manager\dazzle', 'AppDataDirectory', AppDataPath) then
          Result := AppDataPath;
end;

function GetDAZzleLayoutFolder(Value: string): string;
var
  LayoutPath: string;
begin
  // initialize default path, which will be returned when the following registry
  // key queries fail due to missing keys or for some different reason
  Result := 'C:\Users\Public\Documents\Endicia\DAZzle';
  // query the first registry value; if this succeeds, return the obtained value
  if RegQueryStringValue(HKCU, 'Software\Envelope Manager\dazzle\Preferences', 'LayoutDirectory', LayoutPath) then
    Result := LayoutPath
  // otherwise the first registry key query failed, so...
  // query the second registry value; if it succeeds, return the obtained value
  else
    if RegQueryStringValue(HKLM, 'Software\Envelope Manager\DAZzle', 'UserDataDirectory', LayoutPath) then
      Result := LayoutPath;
end;

