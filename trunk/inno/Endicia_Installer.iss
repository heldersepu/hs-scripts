function GetDAZzleInstallFolder(Value: string): string;
var
  InstallPath: string;
begin
  // initialize default path, which will be returned when the following registry
  // key queries fail due to missing keys or for some different reason
  Result := 'C:\Program Files\Envelope Manager\DAZzle'
  // query the first registry value; if this succeeds, return the obtained value
  if RegQueryStringValue(HKCU, 'Software\Envelope Manager\dazzle\Preferences', 'DazzleDirectory', InstallPath) then
    Result := InstallPath;
end;

